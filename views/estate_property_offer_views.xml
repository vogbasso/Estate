<odoo> 
    <record id="estate_property_offer_tree_view" model="ir.ui.view">
        <field name="name">estate_property_offer.model.tree</field>
        <field name="model">estate_property_offer.model</field>
        <field name="arch" type="xml">
            <tree string="Channel">
                <field name = "price"/>
                <field name = "partner_id"/>
                <field name="status"/>
                <field name="create_date"/>
                <field name="validity"/>
                <field name="date_deadline"/>
            </tree>
        </field>
    </record>
    <record id="estate_property_offer_action" model="ir.actions.act_window">
        <field name="name">Estate property Offer</field>
        <field name="res_model">estate_property_offer.model</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>