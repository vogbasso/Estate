    <record id="estate_model_form_view" model="ir.ui.view">
        <field name="name">estate_property.model.form</field>
        <field name="model">estate_property.model</field>
        <field name="arch" type="xml">
            <form string="Estate Forme">
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                        </group>
                        <group>
                            <field name="selling_price"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    
    <record id="estate_model_tree_view" model="ir.ui.view">
        <field name="name">estate_property.model.tree</field>
        <field name="model">estate_property.model</field>
        <field name="arch" type="xml">
            <tree string="Channel">
                <field name="name"/>
                <field name="description"/>
                <field name="selling_price"/>
                <field name="property_type_id"/>
                <field name="salesperson_id"/>
                <field name="tag_ids"/>
                <field name="buyer_id"/>
            </tree>
        </field>
    </record>
  
    <record id="estate_property_model_action" model="ir.actions.act_window">
        <field name="name">Estate property</field>
        <field name="res_model">estate_property.model</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>